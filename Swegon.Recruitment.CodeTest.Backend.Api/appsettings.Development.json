{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AppSettings": {
    "Features": {
      "EnableCaching": true,
      "EnableDetailedErrors": true,
      "EnableSwagger": true
    }
  },
  "Database": {
    "EnableSensitiveDataLogging": true
  }
}
